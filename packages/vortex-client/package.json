{
  "name": "vortex2",
  "description": "A procedural texture generator.",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viridia/vortex.git"
  },
  "keywords": [
    "webgl",
    "texture",
    "generator",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/viridia/vortex/issues"
  },
  "scripts": {
    "start": "REACT_APP_LOCAL=true react-scripts start",
    "start-local": "SERVER_URL=http://localhost:9001/ REACT_APP_LOCAL=true react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --ext .ts --ext .tsx ./src",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "sort-imports-es6-autofix"
    ],
    "rules": {
      "sort-imports-es6-autofix/sort-imports-es6": [
        "warn",
        {
          "ignoreCase": false,
          "ignoreMemberSort": false,
          "memberSyntaxSortOrder": [
            "single",
            "multiple",
            "all",
            "none"
          ]
        }
      ]
    }
  },
  "proxy": "http://localhost:9001",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@reach/dialog": "^0.11.0",
    "axios": "^0.20.0",
    "bind-decorator": "^1.0.11",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "dateformat": "^3.0.3",
    "downloadjs": "^1.4.7",
    "float-hsl2rgb": "^1.0.2",
    "float-rgb2hsl": "^1.0.1",
    "history": "^5.0.0",
    "hotkeys-js": "^3.8.1",
    "marked": "^1.1.1",
    "mobx": "^5.15.6",
    "mobx-react": "^6.2.5",
    "polished": "^3.6.5",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-scripts": "^3.4.3",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.10",
    "@types/dateformat": "^3.0.1",
    "@types/downloadjs": "^1.4.2",
    "@types/jest": "^24.9.1",
    "@types/marked": "^1.1.0",
    "@types/node": "^12.12.54",
    "@types/qs": "^6.9.4",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-env": "^1.15.2",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "http-proxy-middleware": "^1.0.5"
  }
}
